{
  "name": "prism-flow-agent",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "dev": "tsx --watch src/index.ts",
    "dev:frontend": "npm --prefix frontend run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:frontend\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "build": "tsc && node scripts/copy-assets.js",
    "build:frontend": "npm --prefix frontend run build",
    "build:all": "npm run build && npm run build:frontend",
    "serve": "node dist/index.js",
    "prod": "npm run build:all && npm run serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.990.0",
    "@fastify/cors": "^11.2.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/multipart": "^9.4.0",
    "@fastify/static": "^9.0.0",
    "@modelcontextprotocol/sdk": "^1.0.1",
    "@octokit/rest": "^22.0.1",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.5",
    "dotenv": "^17.2.3",
    "fastify": "^5.7.2",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.3.3",
    "mime-types": "^3.0.2",
    "node-cron": "^4.2.1",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "rss": "^1.2.2",
    "rss-parser": "^3.13.0",
    "sharp": "^0.34.5",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typeid-js": "^1.2.0",
    "typescript": "^5.9.3",
    "undici": "^7.22.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/fs-extra": "^11.0.4",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^25.1.0",
    "@types/rss": "^0.0.32",
    "@types/sqlite3": "^3.1.11",
    "concurrently": "^9.2.1",
    "glob": "^13.0.0"
  }
